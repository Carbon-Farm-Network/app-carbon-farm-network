{
  "name": "ui",
  "version": "0.1.0",
  "scripts": {
    "start": "vite dev --clearScreen false --port $UI_PORT",
    "build": "npm run check; vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "package": "npm run build && node zip.js",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@holochain/client": "^0.12.2",
    "@msgpack/msgpack": "^2.7.2",
    "@tailwindcss/forms": "^0.5.3",
    "@vaadin/date-time-picker": "^23.3.13",
    "@valueflows/vf-graphql": "^0.9.0-alpha.9",
    "@vf-ui/graphql-client-holochain": "^0.0.1-alpha.21",
    "@valueflows/vf-graphql-holochain": "^0.0.1-alpha.21",
    "date-picker-svelte": "^2.4.3",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "leaflet": "^1.9.3",
    "svelte-apollo": "^0.5.0",
    "svelte-leafletjs": "^0.10.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^4.0.0",
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.16.3",
    "@sveltejs/vite-plugin-svelte": "^2.2.0",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vf-ui/graphql-client-mock": "^0.0.9",
    "autoprefixer": "^10.4.14",
    "bestzip": "^2.2.0",
    "cssnano": "^6.0.0",
    "eslint": "^8.40.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.23",
    "postcss-import": "^15.1.0",
    "postcss-load-config": "^4.0.1",
    "postcss-preset-env": "^8.3.0",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "svelte": "^3.59.1",
    "svelte-check": "^3.3.2",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.3.5",
    "vitest": "^0.25.3"
  },
  "type": "module"
}
